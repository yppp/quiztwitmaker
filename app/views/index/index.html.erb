<% unless session[:oauth] %>
  <%= form_tag :oauth, method: :get do |f| %>
    <%= submit_tag '認証する' %>
  <% end %>
<% else %>
<% unless @db_userinfo.nil? then %>
<%="ようこそ" + @userinfo.name + "さん"%>


<% @db_userinfo.quizzes.each do |elem| %>
<dl>
  <dt><%= elem.title %></dt>
  <dd><%= elem.text %></dd>
</dl>
<% end %>
<% end %>


<% end %>

